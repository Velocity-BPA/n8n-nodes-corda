{
  "name": "n8n-nodes-corda",
  "version": "1.0.0",
  "description": "n8n community node for R3 Corda distributed ledger platform - Interact with Corda nodes, manage vault states, execute flows, handle tokens and accounts",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "corda",
    "r3",
    "blockchain",
    "dlt",
    "distributed-ledger",
    "enterprise-blockchain",
    "cordapp",
    "vault",
    "flows",
    "tokens",
    "accounts"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://velobpa.com",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-corda.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CordaNodeCredentials.credentials.js",
      "dist/credentials/CordaNetworkCredentials.credentials.js",
      "dist/credentials/Corda5RestApiCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Corda/Corda.node.js",
      "dist/nodes/Corda/CordaTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
